<% layout('/layouts/admin') -%>

<div class="row">
    <div class="form-group">
        <label>Approved</label>
        <p class="form-control-static"><%= item.approved %></p>
    </div>
    <div class="form-group">
        <label>Label</label>
        <p class="form-control-static"><%= item.label %></p>
    </div>
    <div class="form-group">
        <label>Brand</label>
        <p class="form-control-static"><%= item.brand || '' %></p>
    </div>
    <div class="form-group">
        <label>Meta Description</label>
        <p class="form-control-static"><%= item.meta_description || '' %></p>
    </div>
    <div class="form-group">
        <label>Category</label>
        <p class="form-control-static"><%= item.category %></p>
    </div>
</div>
